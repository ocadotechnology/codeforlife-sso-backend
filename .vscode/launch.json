{
  "configurations": [
    {
      "console": "integratedTerminal",
      "justMyCode": false,
      "name": "Python: Current File",
      "program": "${file}",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "env": {
        "PYTEST_ADDOPTS": "--no-cov"
      },
      "justMyCode": false,
      "name": "Pytest",
      "presentation": {
        "hidden": true
      },
      "request": "test",
      "type": "debugpy"
    },
    {
      "args": [
        "runserver",
        "localhost:8001"
      ],
      "django": true,
      "env": {
        "SERVICE_PORT": "8001"
      },
      "justMyCode": false,
      "name": "Django Server",
      "preLaunchTask": "setup",
      "program": "${workspaceFolder}/manage.py",
      "request": "launch",
      "type": "debugpy"
    }
  ],
  "version": "0.2.0"
}